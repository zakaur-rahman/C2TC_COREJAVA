package C2TC;

import java.util.Scanner;

public class forLoop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the number of rows: ");
		int row = sc.nextInt();
		for (int i = 0; i < row; i++) {
			for (int j = 0; j < row; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
		sc.close();

	}

}
